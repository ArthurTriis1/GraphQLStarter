{
  "name": "backend-type-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Arthur Andrade",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch ./index.ts",
    "build": "tsup ./index.ts",
